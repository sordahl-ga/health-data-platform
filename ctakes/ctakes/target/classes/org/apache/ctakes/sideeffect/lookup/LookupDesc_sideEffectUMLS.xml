				<luceneImpl/>
			</implementation>
			<lookupField fieldName="first_word"/>
			<excludeList>
				 <child value="program" />
			   <child value="today" />
			   <child value="nicotine" />
			   <child value="alcohol"/>
			   <child value="allergies"/>
