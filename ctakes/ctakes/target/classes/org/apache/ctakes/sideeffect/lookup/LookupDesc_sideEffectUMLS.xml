			<metaFields>
				<metaField fieldName="cui"/>
				<metaField fieldName="tui"/>
				<metaField fieldName="text"/>
			</metaFields>
		</dictionary>
		<dictionary id="DICT_RXNORM" externalResourceKey="RxnormIndexReader" caseSensitive="false">
